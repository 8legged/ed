<table>
  <tr>
    <th>User Name</th>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Action</th>
  </tr>
  <% @user.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.firstname %></td>
    <td><%= user.lastname %></td>
    <td><%= link_to 'view profile', user %></td>  </tr>
  <% end %>
</table>